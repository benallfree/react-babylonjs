{
  "name": "transpile-ts-loader",
  "version": "0.0.1",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "targets": {
    "default": {
      "source": "./src/index.ts",
      "context": "node",
      "engines": {
        "node": ">12"
      },
      "outputFormat": "commonjs",
      "isLibrary": true,
      "optimize": false
    }
  },
  "license": "MIT",
  "scripts": {
    "build": "parcel build",
    "watch": "chokidar 'src/*' -c 'yarn build'"
  },
  "dependencies": {
    "@types/loader-utils": "^2.0.3",
    "@types/webpack": "^5.28.0",
    "loader-utils": "^3.2.0",
    "typescript": "^4.5.4"
  },
  "peerDependencies": {
    "gatsby": "^4.0",
    "react-codesandbox-devtool": "*"
  },
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@types/prettier": "^2.4.3",
    "babel-loader": "^8.2.3",
    "parcel": "^2.2.0"
  }
}
